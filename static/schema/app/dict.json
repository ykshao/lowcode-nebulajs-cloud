                            "level": "link",
                            "dialog": {
                                "title": "编辑",
                                "body": [
                                    {
                                        "type": "form",
                                        "labelWidth": 85,
                                        "api": {
                                            "method": "post",
                                            "url": "cloud/app/app-dict",
                                            "dataType": "json",
                                            "data": {
                                                "&": "$$",
                                                "id": "${id}"
                                            },
                                            "messages": {
                                                "success": "编辑字典成功"
                                            }
                                        },
                                        "body": [
                                            {
                                                "name": "id",
                                                "label": "ID",
                                                "type": "input-text",
                                                "visibleOn": "",
                                                "clearValueOnHidden": false,
                                                "visible": false
                                            },
                                            {
                                                "type": "input-text",
                                                "name": "code",
                                                "label": "编码",
                                                "autoComplete": {
                                                    "method": "get",
                                                    "url": "cloud/app/app-dict/search/term?word=$term",
                                                    "data": {
                                                        "word": "$code"
                                                    }
                                                },
                                                "size": "md",
                                                "validations": {
                                                    "matchRegexp": "^[A-Z0-9_]+$"
                                                },
                                                "validationErrors": {
                                                    "matchRegexp": "编码由大写字母、下划线和数字组成"
                                                },
                                                "required": true,
                                                "description": "字典编码，通常为大写字母、下划线和数字"
                                            },
                                            {
                                                "label": "值",
                                                "name": "value",
                                                "type": "input-text",
                                                "required": true,
                                                "size": "md"
                                            },
                                            {
                                                "label": "显示",
                                                "name": "label",
                                                "type": "input-text",
                                                "validations": {},
                                                "required": true,
                                                "size": "md"
                                            },
                                            {
                                                "label": "顺序",
                                                "name": "seq",
                                                "type": "input-text",
                                                "validations": {
                                                    "isInt": true
                                                },
                                                "mode": "",
                                                "size": "xs"
                                            },
                                            {
                                                "type": "textarea",
                                                "name": "remark",
                                                "label": "备注",
                                                "size": "lg"
                                            }
                                        ]
                                    }
                                ],
                                "type": "dialog"
                            }
                        },
                        {
                            "type": "button",
                            "label": "删除",
                            "icon": "nb-icon nb-icon-shanchu",
                            "actionType": "ajax",
                            "level": "link",
                            "className": "text-danger",
                            "confirmText": "确定要删除？",
                            "api": {
                                "method": "delete",
                                "url": "cloud/app/app-dict/$id",
                                "messages": {
                                    "success": "删除字典成功"
                                }
                            }
                        }
                    ]
                }
            ],
            "syncLocation": false,
            "api": {
                "method": "get",
                "url": "cloud/app/app-dict",
